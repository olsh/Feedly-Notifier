{
  "name": "feedly-notifier",
  "description": "Feedly notifier is a tiny extension that keeps you up to date with your feedly subscriptions.",
  "author": "Dmitriy Samodov, Oleg Shevchenko",
  "bugs": {
    "url": "https://github.com/olsh/Feedly-Notifier/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olsh/Feedly-Notifier.git"
  },
  "scripts": {
    "build": "set NODE_ENV=production&& node utils/build.js",
    "start": "node utils/webserver.js",
    "sandbox": "npm run start -- --sandbox",
    "dev:chrome": "npm run sandbox -- --browser=chrome",
    "dev:firefox": "npm run sandbox -- --browser=firefox",
    "dev:opera": "npm run sandbox -- --browser=opera"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "echo-loader": "^0.0.1",
    "eslint": "^4.9.0",
    "eslint-loader": "^1.9.0",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "preprocess": "^3.1.0",
    "preprocess-loader": "^0.2.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1",
    "write-file-webpack-plugin": "^4.2.0",
    "yargs": "^9.0.1",
    "zip-webpack-plugin": "^2.0.0"
  },
  "dependencies": {
    "dompurify": "^1.0.2",
    "jquery": "^3.2.1",
    "mustache": "^2.3.0",
    "timeago": "^1.6.1",
    "webextension-polyfill": "^0.2.1"
  }
}
