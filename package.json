{
  "name": "chrome-extension-webpack",
  "version": "0.0.1",
  "description": "A boilerplate to chrome extension with webpack",
  "scripts": {
    "build": "set NODE_ENV=production&& node utils/build.js",
    "start": "node utils/webserver.js",
    "dev:chrome": "npm run start -- --browser=chrome",
    "dev:firefox": "npm run start -- --browser=firefox",
    "dev:opera": "npm run start -- --browser=opera"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "echo-loader": "^0.0.1",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "preprocess": "^3.1.0",
    "preprocess-loader": "^0.2.2",
    "style-loader": "^0.13.0",
    "webpack": "3.6.0",
    "webpack-dev-server": "^2.9.1",
    "write-file-webpack-plugin": "^4.2.0",
    "yargs": "^9.0.1",
    "zip-webpack-plugin": "^2.0.0"
  },
  "dependencies": {
    "dompurify": "^1.0.2",
    "jquery": "^3.2.1",
    "mustache": "^2.3.0",
    "timeago": "^1.6.1",
    "webextension-polyfill": "^0.2.0"
  }
}
