    "@types/chrome": "^0.0.269",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-timeago": "^4.1.7",
    "@types/webextension-polyfill": "^0.12.1",
    "@types/webpack": "^5.28.5",
    "copy-webpack-plugin": "^12.0.2",
